<h1>Listing passes</h1>

<table>
  <tr>
    <th>Name</th>
    <th>Locality</th>
    <th>Altitude</th>
    <th>Latitude</th>
    <th>Longitude</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @passes.each do |pass| %>
  <tr>
    <td><%= pass.name %></td>
    <td><%= pass.locality %></td>
    <td><%= pass.altitude %></td>
    <td><%= pass.latitude %></td>
    <td><%= pass.longitude %></td>
    <td><%= link_to 'Show', pass %></td>
    <td><%= link_to 'Edit', edit_pass_path(pass) %></td>
    <td><%= link_to 'Destroy', pass, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Pass', new_pass_path %>
